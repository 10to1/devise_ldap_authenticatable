<!DOCTYPE html>
<html>
<head>
  <title>RailsApp</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  
<% flash.each do |name, message| %>
  <p id="<%= name %>" class="flash"><%= message %></p>
<% end %>

<%= yield %>

</body>
</html>
